{
  "name": "rainbow-palette",
  "version": "0.2.2",
  "author": {
    "name": "pawcode Development"
  },
  "bugs": {
    "url": "https://github.com/pawcoding/Rainbow-Palette/issues"
  },
  "description": "Rainbow Palette is a color palette generator that allows you to create beautiful color palettes and export them in different formats.",
  "homepage": "https://colos.apps.pawcode.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawcoding/Rainbow-Palette.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prepwa": "pnpm run build",
    "pwa": "npx http-server -p 4242 -c-1 dist/rainbow-palette",
    "prebuild": "pnpm run build:sitemap",
    "build": "ng build",
    "build:sitemap": "node scripts/update-sitemap.js",
    "build:storybook": "ng run rainbow-palette:build-storybook",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:watch": "nodemon --exec \"pnpm run lint || exit 1\" --ext ts,html,css",
    "prettier": "npx prettier --write src/",
    "prettier:check": "npx prettier --check src/",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless,FirefoxHeadless --code-coverage --progress=false",
    "docs": "compodoc -p ./tsconfig.json -s",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run rainbow-palette:storybook",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged",
    "preinstall": "npx only-allow pnpm"
  },
  "private": true,
  "lint-staged": {
    "**/*": "npx prettier --write src/ --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "^16.1.6",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@angular/service-worker": "^16.1.6",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "ngx-matomo-client": "^5.0.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1",
    "uuid": "^9.0.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.5",
    "@angular/compiler-cli": "^16.1.6",
    "@babel/core": "^7.22.9",
    "@compodoc/compodoc": "^1.1.21",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@storybook/addon-actions": "^7.1.1",
    "@storybook/addon-docs": "^7.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/angular": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/forms": "^0.5.4",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.4.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "chromatic": "^6.20.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.1",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6"
  },
  "readme": "https://github.com/pawcoding/Rainbow-Palette#readme",
  "_id": "rainbow-palette@0.2.1"
}
